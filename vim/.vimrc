" set hls ic is nu noswf
colorscheme peachpuff
filetype plugin indent on
set autoindent
set autoread
set backspace=indent,eol,start
set clipboard+=unnamedplus
set expandtab
set hidden
set hlsearch
set ignorecase
set incsearch
set lazyredraw
set mouse=a
set noerrorbells
set noswapfile
set novisualbell
set nowrap
set number
set relativenumber
set shiftwidth=4
set smartcase
set smartindent
set smarttab
set softtabstop=4
set splitbelow
set splitright
set tabstop=4
set timeoutlen=500
set title
set titlestring=vim\ %t\ %M\ (%F)
set ttimeout
set ttimeoutlen=10
syntax on
