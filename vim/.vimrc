unlet! skip_defaults_vim
source $VIMRUNTIME/defaults.vim

" set hls ic is nu noswf
set hlsearch
set ignorecase
set incsearch
set number
set noswapfile
                               
set autoread
set backspace=indent,eol,start
set laststatus=2
set lazyredraw
set relativenumber
set ruler
set wildmenu
                               
filetype plugin indent on
syntax on
