{
    "cSpell.allowCompoundWords": true,
    "coc.source.emoji.filetypes": ["markdown", "gitcommit"],
    "diagnostic-languageserver.filetypes": {
        "dockerfile": "hadolint",
        "sh": "shellcheck"
    },
    "diagnostic.errorSign": "x",
    "diagnostic.hintSign": "h",
    "diagnostic.infoSign": "i",
    "diagnostic.warningSign": "!",
    "explorer.autoExpandOptions": ["recursive", "recursiveSingle"],
    "explorer.file.root.template": "[icon] [git & 1] [hidden & 1][root]",
    "explorer.file.showHiddenFiles": true,
    "explorer.git.icon.status.ignored": "i",
    "explorer.git.icon.status.mixed": "●",
    "explorer.icon.enableNerdfont": true,
    "explorer.keyMappings.global": {
        "<cr>": ["expandable?", "expand", "open:select:keep"],
        "o": ["expandable?", "expand", "open:select:keep"],
        "v": "open:vsplit"
    },
    "explorer.width": 30,
    "git.addedSign.hlGroup": "GitGutterAdd",
    "git.addedSign.text": "┃",
    "git.changeRemovedSign.hlGroup": "GitGutterChangeDelete",
    "git.changeRemovedSign.text": "┃",
    "git.changedSign.hlGroup": "GitGutterChange",
    "git.changedSign.text": "┃",
    "git.removedSign.hlGroup": "GitGutterDelete",
    "git.removedSign.text": "┃",
    "git.topRemovedSign.hlGroup": "GitGutterDelete",
    "git.topRemovedSign.text": "┃",
    "markdownlint.config": {
        "MD013": false,
        "MD024": false,
        "MD030": false
    },
    "prettier.tabWidth": 4,
    "pyright.organizeimports.provider": "isort",
    "python.formatting.blackArgs": ["--line-length=88"],
    "python.formatting.provider": "black",
    "sh.highlightParsingErrors": false,
    "snippets.extends": {
        "javascriptreact": ["javascript"],
        "typescript": ["javascript"],
        "typescriptreact": ["javascript"]
    },
    "suggest.completionItemKindLabels": {
        "class": "  (class)",
        "color": "  (color)",
        "constant": "  (constant)",
        "constructor": "  (constructor)",
        "default": "  (default)",
        "enum": " 練 (enum)",
        "enumMember": "  (enum-member)",
        "event": "  (event)",
        "field": "  (field)",
        "file": "  (file)",
        "folder": "  (folder)",
        "function": "  (function)",
        "interface": "  (interface)",
        "keyword": "  (keyword)",
        "method": "  (method)",
        "module": "  (module)",
        "operator": "  (operator)",
        "property": "  (property)",
        "reference": "  (reference)",
        "snippet": "  (snippet)",
        "struct": "  (struct)",
        "text": "  (text)",
        "typeParameter": "<>(type-parameter)",
        "unit": "  (unit)",
        "value": "  (value)",
        "variable": "[](variable)"
    },
    "suggest.removeDuplicateItems": true,
    "tabnine.priority": 60,
    "tailwindCSS.emmetCompletions": true,
    "tailwindCSS.experimental.classRegex": [
        "class[\\w]*\\s*=\\s*[{\"'`]+(.*)[\"'`}]+"
    ],
    "tailwindCSS.experimental.showPixelValues": true,
    "yank.highlight.duration": 200,
    "yank.priority": 50
}
